pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo 'I am in Build Phase'
            }
        }
        
        stage('Test') {
            steps {
                echo 'I am in Test Phase'
            }
        }
        
        stage('Deploy') {
            steps {
                echo 'I am in Deploy Phase'
            }
        }
        
         stage('Monitoring') {
            steps {
                echo 'I am in Monitoring Phase'
            }
        }
        stage('VersionCheck') {
            steps {
                echo 'I am in Version Phase'
                sh 'aws s3 ls'
            }
        }
    }
}
